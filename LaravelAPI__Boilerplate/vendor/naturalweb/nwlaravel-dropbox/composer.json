{
    "name": "naturalweb/nwlaravel-dropbox",
    "description": "Dropbox SDK Service Provider for the Laravel PHP Framework",
    "keywords": ["sdk-dropbox", "laravel", "php", "dropbox"],
    "type": "library",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/naturalweb/NwLaravel-Dropbox",
    "require": {
        "php": ">=5.3.0",
        "laravel/framework": "~4.1",
        "dropbox/dropbox-sdk": "1.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*",
        "mockery/mockery": "0.7.2",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "NwLaravel": "src/"
        }
    },
    "suggest": {
        "naturalweb/nwsilex": ">=0.1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    }
}

